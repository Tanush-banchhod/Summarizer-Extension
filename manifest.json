{
    "manifest_version": 3,
    "name": "Webpage Summarizer",
    "version": "1.0",
    "description": "Summarizes the content of webpages.",
    "permissions": ["activeTab", "scripting"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    }
  }
  